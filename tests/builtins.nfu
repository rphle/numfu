import * from "math"
import * from "std"
import * from "types"

5 + 3                      ---> $ ==  8
"Hello" + " " + "World"    ---> $ ==  "Hello World";
[1, 2] + [3, 4]            ---> $ ==  [1, 2, 3, 4]

5 * 3               ---> $ == 15
"Ha" * 3            ---> $ == "HaHaHa"
3 * "Ho"            ---> $ == "HoHoHo";
[1, 2] * 3          ---> $ == [1, 2, 1, 2, 1, 2]

max(1, 5, 3, 9, 2)  ---> $ == 9
max([1, 5, 3, 9, 2]) ---> $ == 9
min(1, 5, 3, 9, 2)  ---> $ == 1
min([1, 5, 3, 9, 2]) ---> $ == 1

Bool(1)             ---> $
Bool(0)             ---> !$
Bool("hello")       ---> $
Bool("")            ---> !$
Bool([1, 2])        ---> $
Bool([])            ---> !$

Number(true)        ---> $ == 1
Number(false)       ---> $ == 0
Number("42")        ---> $ == 42
Number("3.14")      ---> $ == 3.14

List("hello")       ---> $ == ["h", "e", "l", "l", "o"]
List([1, 2, 3])     ---> $ == [1, 2, 3]

append([1, 2, 3], 4)        ---> $ == [1, 2, 3, 4]
append([], "first")         ---> $ == ["first"]

length([1, 2, 3, 4])        ---> $ == 4
length("hello")             ---> $ == 5
length([])                  ---> $ == 0

contains([1, 2, 3], 2 )     ---> $
contains("hello", "lo")     ---> $
contains([1, 2, 3], 4)      ---> !$

set([1, 2, 3], 1, 99)       ---> $ == [1, 99, 3]
set("hello", 0, "H")        ---> $ == "Hello"

reverse([1, 2, 3, 4])       ---> $ == [4, 3, 2, 1]
reverse("hello")            ---> $ == "olleh"

sort([3, 1, 4, 1, 5])       ---> $ == [1, 1, 3, 4, 5]
sort("dcba")                ---> $ == "abcd"

slice([1, 2, 3, 4, 5], 1, 3)    ---> $ == [2, 3, 4]
slice("hello", 1, 3)            ---> $ == "ell"
slice([1, 2, 3, 4, 5], 2, -2)   ---> $ == [3, 4]

join(["hello", "world"], "-")   ---> $ == "hello-world"
join([], ",")                   ---> $ == ""

split("hello,world", ",")       ---> $ == ["hello", "world"]
split("one two three", " ")     ---> $ == ["one", "two", "three"]
split("hello", "")              ---> $ == ["hello"]

format("Hello, {}!", "World")               ---> $ == "Hello, World!"
format("{} + {} = {}", "2", "3", "5")       ---> $ == "2 + 3 = 5"
format("Name: {}, Age: {}", "Alice", "30")  ---> $ == "Name: Alice, Age: 30"

trim("  hello  ")       ---> $ == "hello"

toLowerCase("HELLO")    ---> $ == "hello"

toUpperCase("hello")    ---> $ == "HELLO"

replace("hello world", "world", "there") ---> $ == "hello there"

count("hello hello", "hello") ---> $ == 2

map([1, 2, 3, 4], {x -> x * x})     ---> $ == [1, 4, 9, 16]
map(["hello", "world"], length)     ---> $ == [5, 5]
map([1, 2, 3], {x -> x + 10})       ---> $ == [11, 12, 13]

filter([1, 2, 3, 4, 5], {x -> x % 2 == 0})                    ---> $ == [2, 4];
filter(["apple", "banana", "cherry"], {s -> length(s) > 5})   ---> $ == ["banana", "cherry"];

0 / 0    ---> isnan($);
sqrt(-1) ---> isnan($);

1 / 0    ---> isinf($);
-1 / 0   ---> isinf($);
