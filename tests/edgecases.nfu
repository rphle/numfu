import * from "math"

max(_)([1,2,3]) ---> $ == 3;

max(1,_,_)(2)(3) ---> $ == 3;

{a, b, c -> a+b+c}(_, 5, _);
// {a,c -> a+5+c}

{a,b,c->a+b+c}(5, _);
// {b,c -> 5+b+c}

{a,b,...c -> a+b+c[0]}(_, 5, 2);
// {a -> a+5+[2][0]}

{a,...c -> a+c[0]}(10, _, 20);
// {c -> 10+[ _ , 20 ][1]}

({a -> {b -> a*b}})(2);
// {b -> 2*b}
